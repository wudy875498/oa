<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD 
 Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<!--  TIMESTAMP -->
<!-- namespace为接口的名字 -->
<mapper namespace="com.cao.oa.mapper.UserKindMapper">

	<select id="getAllUserKind" resultType="com.cao.oa.bean.UserKind">
		select * from user_kind
	</select>
	
	<select id="getNameById" resultType="String">
		select name from user_kind where id = #{id}
	</select>





</mapper>